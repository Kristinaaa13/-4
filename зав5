# 5) Дизайн замкнутої черги — ті ж ідеї, спрощений інтерфейс
class MyCircularQueue:
    def __init__(self, k: int):
        self.n = k
        self.a = [0] * k
        self.size = 0
        self.front = 0

    def enQueue(self, value: int) -> bool:
        if self.isFull(): return False
        idx = (self.front + self.size) % self.n
        self.a[idx] = value
        self.size += 1
        return True

    def deQueue(self) -> bool:
        if self.isEmpty(): return False
        self.front = (self.front + 1) % self.n
        self.size -= 1
        return True

    def Front(self) -> int:
        return -1 if self.isEmpty() else self.a[self.front]

    def Rear(self) -> int:
        return -1 if self.isEmpty() else self.a[(self.front + self.size - 1) % self.n]

    def isEmpty(self) -> bool:
        return self.size == 0

    def isFull(self) -> bool:
        return self.size == self.n
# Складність: O(1) на операцію, пам'ять O(k).
